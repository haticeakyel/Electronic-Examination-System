<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="386.0" prefWidth="552.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="exam.LoginController">
   <children>
      <Label layoutX="144.0" layoutY="147.0" text="Username" />
      <Label layoutX="144.0" layoutY="185.0" text="Password" />
      <TextField fx:id="userName" layoutX="230.0" layoutY="143.0" promptText="username" />
      <Button fx:id="submitButton" layoutX="230.0" layoutY="229.0" mnemonicParsing="false" onAction="#login" text="Log In" />
      <Button fx:id="exitStudentLogin" layoutX="33.0" layoutY="35.0" mnemonicParsing="false" onAction="#exitStudentLoginBtn" text="Back" />
      <PasswordField fx:id="password" layoutX="230.0" layoutY="181.0" promptText="password" />
   </children>
</AnchorPane>
